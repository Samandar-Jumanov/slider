{
  "name": "express-typescript-boilerplate",
  "version": "1.0.14",
  "description": "An Express boilerplate backend",
  "author": "Edwin Hernandez",
  "repository": "edwinhern/express-typescript-2024",
  "license": "MIT",
  "main": "server.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/server.ts | pino-pretty",
    "build": "tsup",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --fix",
    "format": "biome format src/",
    "test": "vitest run",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "@langchain/community": "^0.3.16",
    "@langchain/core": "^0.3.19",
    "@langchain/openai": "^0.3.14",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.1",
    "express-rate-limit": "^7.2.0",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "image-size": "^1.1.1",
    "libreoffice-convert": "^1.6.0",
    "mammoth": "^1.8.0",
    "openai": "^4.73.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pino": "^9.4.0",
    "pino-http": "^10.0.0",
    "pptxgenjs": "^3.12.0",
    "swagger-ui-express": "^5.0.0",
    "telegraf": "^4.16.3",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@types/pdf-parse": "^1.1.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pino-pretty": "^11.0.0",
    "rimraf": "^6.0.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --apply --no-errors-on-unmatched"
    ]
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
